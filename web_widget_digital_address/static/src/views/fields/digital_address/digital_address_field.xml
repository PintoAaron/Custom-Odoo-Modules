<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">

    <t t-name="web.DigitalAddressField">
        <t t-if="props.readonly">
            <span t-esc="props.record.data[props.name]" />
        </t>
        <t t-else="">
            <input
                class="o_input"
                t-att-id="props.id"
                type="text"
                autocomplete="off"
                t-att-placeholder="props.placeholder"
                t-ref="input"
            />
        </t>
    </t>

</templates>
